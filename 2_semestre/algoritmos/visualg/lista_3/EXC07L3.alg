algoritmo "semnome"
// Função :
// Autor :
// Data : 16/08/2023
// Seção de Declarações 
var
n,media:real
nn:inteiro
inicio
media<-0
para nn de 1 ate 15 faca
     escreva("informe o ",nn,"° numero: ")
     leia(n)
     media<-media+n
fimpara
media<-(media/15)
escreva("a media de todos os 15 numero é : ",media)
fimalgoritmo
