algoritmo "semnome"
// Função :
// Autor :
// Data : 07/08/2023
// Seção de Declarações 
var
n1,n2:real
inicio
escreva("informe um numero:  ")
leia(n1)
escreva("informe um outro numero:  ")
leia(n2)
se (n1>n2) entao
   escreva(n1," é maior que ",n2)
senao
     se (n1=n2) entao
     escreva(n1," é igual a ",n2)
     senao
     escreva(n2," é maior que ",n1)
     fimse
fimse
fimalgoritmo
