algoritmo "semnome"
// Função :
// Autor :
// Data : 07/08/2023
// Seção de Declarações 
var
nome:caractere
idade,buxa:caractere
inicio
escreva("informe seu nome:  ")
leia(nome)
escreva("informe sia idade:  ")
leia(idade)
se (idade>="18") entao
   escreva(nome," voce esta apto para tirar cnh")
senao
     escreva(nome," voce nao esta apto para tirar cnh")
fimse
fimalgoritmo
